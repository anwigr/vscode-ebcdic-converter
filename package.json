{
  "name": "ebcdic-converter",
	"displayName": "EBCDIC Converter",
	"description": "A simple Visual Studio Code extension to convert IBM EBCDIC to other more common character encodings such as UTF-8.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:ebcdic-converter.helloWorld"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
            "command": "ebcdic-converter.helloWorld",
            "title": "Hello World"

		}]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.52.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.11.7",
        "eslint": "^7.15.0",
        "glob": "^7.1.6",
        "mocha": "^8.1.3",
        "typescript": "^4.1.2",
        "vscode-test": "^1.4.1"
    }

}
